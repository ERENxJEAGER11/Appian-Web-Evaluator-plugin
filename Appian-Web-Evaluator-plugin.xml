<?xml version="1.0" encoding="UTF-8"?>
<appian-component-plugin name="Web Evaluator Plug-in" key="com.codxman.web.evaluator">
  <plugin-info>
    <description>Provides a web frame to evaluate your javascript code and render on the interface</description>
    <vendor name="codxman" url="" />
    <support supported="true" email="support@appian.com" url="https://github.com/ERENxJEAGER11"/>
    <version>1.0</version>
  </plugin-info>

  <component rule-name="webEvaluator" version="1.0.0">
    <sdk-version>1.0.0</sdk-version>
    <icon-file>google-maps.svg</icon-file>
    <parameter>
      <name>javascript</name>
      <category>input-only</category>
      <type>Text</type>
    </parameter>
    <parameter>
      <name>saveInto</name>
      <category>event</category>
    </parameter>
    <html-entry-point>index.html</html-entry-point>
    <supported-user-agents>chrome firefox ie11 edge safari mobile</supported-user-agents>
  </component>
</appian-component-plugin>